{
  "name": "dot-web-demo",
  "version": "2.1.7",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm -r --parallel dev",
    "web-demo:dev": "pnpm -r --parallel --filter dot-web-demo-server --filter dot-web-demo-client dev",
    "ocr-demo:dev": "pnpm -r --parallel --filter dot-web-demo-server --filter dot-web-ocr-demo-client dev",
    "server:start": "pnpm --filter dot-web-demo-server start",
    "server:build": "pnpm --filter dot-web-demo-server build",
    "web-demo-client:build": "pnpm codegen && pnpm --filter dot-web-demo-client build",
    "ocr-demo-client:build": "pnpm codegen && pnpm --filter dot-web-ocr-demo-client build",
    "codegen": "pnpm -r --parallel codegen",
    "build": "pnpm -r --parallel build",
    "lint": "pnpm -r --parallel lint",
    "lint:fix": "pnpm lint --fix",
    "pr": "pnpm tsc && pnpm lint && pnpm test",
    "test": "vitest run",
    "test:ui": "pnpm -r --parallel test:ui",
    "tsc": "pnpm -r --parallel tsc",
    "bump:version": "VERSION=$v pnpm --r bump:version && npm pkg set version=$v",
    "cy:ocr-demo:run": "cypress run --config-file cypress/cypress.ocrDemo.config.ts --headless --browser chrome",
    "cy:web-demo:run": "cypress run --config-file cypress/cypress.webDemo.config.ts --headless --browser chrome",
    "cy:ocr-demo:open": "cypress open --config-file cypress/cypress.ocrDemo.config.ts",
    "cy:web-demo:open": "cypress open --config-file cypress/cypress.webDemo.config.ts",
    "cy:ocr-demo": "cypress run --config-file cypress/cypress.ocrDemo.config.ts --spec cypress/e2e/ocr-demo",
    "cy:ocr-demo-dev": "cypress run --config-file cypress/cypress.ocrDemo.config.ts --spec cypress/e2e/ocr-demo --config baseUrl='https://dot-dev.innovatrics.com/document-verification'",
    "cy:ocr-demo-stg": "cypress run --config-file cypress/cypress.ocrDemo.config.ts --spec cypress/e2e/ocr-demo --config baseUrl='https://dot-stg.innovatrics.com/document-verification'",
    "cy:web-demo": "cypress run --config-file cypress/cypress.webDemo.config.ts --spec cypress/e2e/web-demo",
    "cy:web-demo-dev": "cypress run --config-file cypress/cypress.webDemo.config.ts --spec cypress/e2e/web-demo --config baseUrl='https://dot-dev.innovatrics.com'",
    "cy:web-demo-stg": "cypress run --config-file cypress/cypress.webDemo.config.ts --spec cypress/e2e/web-demo --config baseUrl='https://dot-stg.innovatrics.com'",
    "cy:ocr-demo-dev-dashboard": "cypress run --config-file cypress/cypress.ocrDemo.config.ts --record --spec cypress/e2e/ocr-demo --key a678a7e2-77cd-4472-b0e3-7953b05fb245 --config baseUrl='https://dot-dev.innovatrics.com/document-verification'",
    "cy:web-demo-dev-dashboard": "cypress run --config-file cypress/cypress.webDemo.config.ts --record --spec cypress/e2e/web-demo --key 0280e86b-e530-4750-9e1e-084c49af6072 --config baseUrl='https://dot-dev.innovatrics.com'",
    "ci:web-demo:cy-run": "start-server-and-test web-demo:dev http://localhost:3001 cy:web-demo:run",
    "ci:ocr-demo:cy-run": "start-server-and-test ocr-demo:dev http://localhost:3002 cy:ocr-demo:run",
    "win-setup": "npm config set script-shell \"C:\\Program Files\\Git\\bin\\bash.exe\"",
    "prepare": "husky"
  },
  "devDependencies": {
    "@dot/eslint-config-react": "^0.0.8",
    "@dot/eslint-config-typescript": "^0.0.8",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitest/ui": "^2.0.4",
    "chai-colors": "^1.0.1",
    "cypress": "^13.13.1",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.4.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "husky": "^9.1.1",
    "jest-canvas-mock": "^2.5.2",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "start-server-and-test": "^2.0.4",
    "typescript": "~5.5.4",
    "vitest": "^2.0.4"
  },
  "packageManager": "pnpm@9.6.0",
  "resolutions": {
    "@types/react": "^18.3.3",
    "graphql": "^16.9.0"
  }
}
