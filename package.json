{
  "name": "dot-web-demo",
  "version": "2.0.3",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn server:dev & yarn web-demo-client:dev & yarn ocr-demo-client:dev",
    "web-demo:dev": "yarn server:dev & yarn web-demo-client:dev",
    "ocr-demo:dev": "yarn server:dev & yarn ocr-demo-client:dev",
    "server:dev": "yarn workspace dot-web-demo-server dev",
    "server:start": "yarn workspace dot-web-demo-server start",
    "server:build": "yarn workspace dot-web-demo-server build",
    "web-demo-client:dev": "yarn workspace dot-web-demo-client dev",
    "web-demo-client:build": "yarn codegen && yarn workspace dot-web-demo-client build",
    "ocr-demo-client:dev": "yarn workspace dot-web-ocr-demo-client dev",
    "ocr-demo-client:build": "yarn codegen && yarn workspace dot-web-ocr-demo-client build",
    "client:codegen": "yarn workspace dot-web-common-client codegen",
    "server:codegen": "yarn workspace dot-web-demo-server codegen",
    "codegen": "yarn client:codegen & yarn server:codegen",
    "build": "yarn workspaces run build",
    "prettier:check": "prettier -l \"packages/**/*.{ts,tsx,js,md}\"",
    "prettier": "prettier  --write \"packages/**/*.{ts,tsx,js,md}\"",
    "lint": "yarn workspaces run lint",
    "lint:fix": "yarn lint --fix",
    "pr": "yarn run tsc && yarn run prettier:check && yarn run lint && yarn test",
    "test": "yarn workspaces run test",
    "test:ui": "yarn workspaces run test:ui",
    "tsc": "yarn workspaces run tsc",
    "cy:ocr-demo:run": "cypress run --config-file cypress/cypress.ocrDemo.config.ts --headless --browser chrome",
    "cy:web-demo:run": "cypress run --config-file cypress/cypress.webDemo.config.ts --headless --browser chrome",
    "cy:ocr-demo:open": "cypress open --config-file cypress/cypress.ocrDemo.config.ts",
    "cy:web-demo:open": "cypress open --config-file cypress/cypress.webDemo.config.ts",
    "cy:ocr-demo": "cypress run --config-file cypress/cypress.ocrDemo.config.ts --spec cypress/e2e/ocr-demo",
    "cy:ocr-demo-dev": "cypress run --config-file cypress/cypress.ocrDemo.config.ts --spec cypress/e2e/ocr-demo --config baseUrl='https://dot-dev.innovatrics.com/document-verification'",
    "cy:ocr-demo-stg": "cypress run --config-file cypress/cypress.ocrDemo.config.ts --spec cypress/e2e/ocr-demo --config baseUrl='https://dot-stg.innovatrics.com/document-verification'",
    "cy:web-demo": "cypress run --config-file cypress/cypress.webDemo.config.ts --spec cypress/e2e/web-demo",
    "cy:web-demo-dev": "cypress run --config-file cypress/cypress.webDemo.config.ts --spec cypress/e2e/web-demo --config baseUrl='https://dot-dev.innovatrics.com'",
    "cy:web-demo-stg": "cypress run --config-file cypress/cypress.webDemo.config.ts --spec cypress/e2e/web-demo --config baseUrl='https://dot-stg.innovatrics.com'",
    "cy:ocr-demo-dev-dashboard": "cypress run --config-file cypress/cypress.ocrDemo.config.ts --record --spec cypress/e2e/ocr-demo --key a678a7e2-77cd-4472-b0e3-7953b05fb245 --config baseUrl='https://dot-dev.innovatrics.com/document-verification'",
    "cy:web-demo-dev-dashboard": "cypress run --config-file cypress/cypress.webDemo.config.ts --record --spec cypress/e2e/web-demo --key 0280e86b-e530-4750-9e1e-084c49af6072 --config baseUrl='https://dot-dev.innovatrics.com'",
    "ci:web-demo:cy-run": "start-server-and-test web-demo:dev http://localhost:3001 cy:web-demo:run",
    "ci:ocr-demo:cy-run": "start-server-and-test ocr-demo:dev http://localhost:3002 cy:ocr-demo:run",
    "postinstall": "husky install",
    "win-setup": "yarn config set script-shell \"C:\\Program Files\\Git\\bin\\bash.exe\""
  },
  "devDependencies": {
    "@innovatrics/eslint-config-typescript-react": "^0.0.8",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitest/ui": "^0.24.3",
    "chai-colors": "^1.0.1",
    "cypress": "^10.10.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.8.4"
  },
  "resolutions": {
    "@types/react": "^18.0.21"
  }
}
