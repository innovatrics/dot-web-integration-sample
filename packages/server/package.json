{
  "name": "dot-web-demo-server",
  "version": "2.0.13",
  "description": "NodeJS GraphQL demo server.",
  "main": "dist/src/server.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:local": "pnpm build && pnpm start",
    "start": "node dist/src/server.js",
    "dev": "pnpm codegen && nodemon src/server.ts",
    "build": "rimraf dist && pnpm codegen && tsc && cp src/graphQL/schema.graphql dist/src/graphQL/schema.graphql",
    "codegen": "graphql-codegen --config ./graphql-codegen.yml",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "tsc": "tsc"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@types/cors": "^2.8.13",
    "axios": "^1.4.0",
    "axios-mock-adapter": "^1.21.5",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.7.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "axios-mock-adapter": "^1.21.5",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  }
}
