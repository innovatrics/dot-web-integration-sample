{
  "name": "dot-web-demo-server",
  "version": "2.0.8",
  "description": "NodeJS GraphQL demo server.",
  "main": "dist/src/server.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:local": "yarn build && yarn start",
    "start": "node dist/src/server.js",
    "dev": "yarn codegen && nodemon src/server.ts",
    "build": "rimraf dist && yarn codegen && tsc && cp src/graphQL/schema.graphql dist/src/graphQL/schema.graphql",
    "codegen": "graphql-codegen --config ./graphql-codegen.yml",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0"
  },
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "@types/cors": "^2.8.13",
    "axios": "^1.2.6",
    "axios-mock-adapter": "^1.21.2",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/typescript": "^2.8.8",
    "@graphql-codegen/typescript-resolvers": "^2.7.13",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "axios-mock-adapter": "^1.21.2",
    "eslint-plugin-graphql": "^4.0.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "vitest": "^0.28.3"
  }
}
